<?xml version="1.0"?>
<!-- fwd tank
Syd Adams
-->
<PropertyList>

    <path>fuel-qty.ac</path>

    <animation>
        <type>material</type>
        <object-name>FuelQTY.face</object-name>
        <object-name>FuelQTY.tank</object-name>
        <object-name>FuelQTY.needle</object-name>
        <emission>
            <red>0.028</red>
            <green>0.014</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>FuelQTY.tank</object-name>
        <property>sim/signals/fdm-initialized</property>
        <factor>0.0755</factor>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FuelQTY.needle</object-name>
        <property>consumables/fuel/tank[0]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>1400</ind><dep>320</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>