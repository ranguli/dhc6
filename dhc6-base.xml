<input>
    <keyboard>
        <key n="127">
            <name>Delete</name>
            <desc>Toggle Reversers</desc>
            <binding>
                <condition>
                    <equals>
                        <property>controls/engines/engine[0]/throttle</property>
                        <value>0</value>
                    </equals>
                </condition>
                <command>property-toggle</command>
                <property>controls/engines/engine[0]/reverser</property>
            </binding>
            <binding>
                <condition>
                    <equals>
                        <property>controls/engines/engine[1]/throttle</property>
                        <value>0</value>
                    </equals>
                </condition>
                <command>property-toggle</command>
                <property>controls/engines/engine[1]/reverser</property>
            </binding>
        </key>
        <key n="79">
            <name>O</name>
            <desc>Open aerotow hook</desc>
            <binding>
                <command>property-assign</command>
                <property>sim/hitches/aerotow/open</property>
                <value type="bool">true</value>
            </binding>
        </key>
        <key n="111">
            <name>o</name>
            <desc>Lock aerotow hook</desc>
            <binding>
                <command>property-assign</command>
                <property>sim/hitches/aerotow/open</property>
                <value type="bool">false</value>
            </binding>
        </key>
	    <key n="106">
            <name>j</name>
            <desc>Push the left condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[0]/condition</property>            <step>0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	    <key n="107">
            <name>k</name>
            <desc>Push the right condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[1]/condition</property>            <step>0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	    <key n="74">
            <name>J</name>
            <desc>Pull the left condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[0]/condition</property>            <step>-0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	    <key n="75">
            <name>K</name>
            <desc>Pull the right condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[1]/condition</property>            <step>-0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
        <!-- Disable the following two key bindings because of the walker:
        <key n="115">
            <name>s</name>
            <desc>Set engine starter left</desc>
            <repeatable type="bool">false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>controls/engines/internal-engine-starter</property>
                <step>1</step>
                <min>-1</min>
                <max>1</max>
            </binding>
        </key>
        <key n="100">
            <name>d</name>
            <desc>Set engine starter right</desc>
            <repeatable type="bool">false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>controls/engines/internal-engine-starter</property>
                <step>-1</step>
                <min>-1</min>
                <max>1</max>
            </binding>
        </key-->
        <key n="102">
            <name>f</name>
            <desc>Toggle flashlight</desc>
            <repeatable type="bool">false</repeatable>
            <binding>
                <command>nasal</command>
                <script>dhc6.toggle_flashlight()</script>
            </binding>
        </key>
        <key n="121">
            <name>y</name>
            <desc>Show/hide yokes</desc>
            <binding>
                <command>property-toggle</command>
                <property>sim/yokes-visible</property>
            </binding>
        </key>
        <key n="267">
            <name>F11</name>
            <desc>Autopilot Controls</desc>
            <binding>
                <command>nasal</command>
                <script>fgcommand("dialog-show", props.Node.new({"dialog-name": "autopilot"}));</script>
            </binding>
        </key>
        <key n="268">
            <name>F12</name>
            <desc>Radio Stack</desc>
            <binding>
                <command>nasal</command>
                <script>fgcommand("dialog-show", props.Node.new({"dialog-name": "tranceivers"}));</script>
            </binding>
        </key>
    	<key n="66">
        	<name>Shift-B</name>
        	<desc>Parking Brakes</desc>
        	<binding>
	            <command>property-toggle</command>
            	<property>/controls/gear/parkingbrake-lever</property>
        	</binding>
    	</key>
    </keyboard>
